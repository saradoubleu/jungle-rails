<% @page_title = "UserAuth | Signup" %>
<div class="Sign_Form">
  <h1>Sign Up</h1>
  <%= form_for(:user, :url => {:controller => 'users', :action => 'create'}) do |f| %>
    <p> First Name:</br> <%= f.text_field :fname%> </p>
    <p> Last Name:</br> <%= f.text_field :lname%> </p>
    <p> Email:</br> <%= f.text_field :email%> </p>
    <p> Password:</br> <%= f.password_field :password%></p>
    <%= f.submit :Signup %>
  <% end %>
</div>
<div class="login">
<h1> Login </h1>
</div>
</div>
</div>

